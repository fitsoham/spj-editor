{
  "name": "spacejoy-v2-dweb",
  "author": "Saurabh Sachan",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "export": "cross-env NEXT_EXPORT=true next build && cross-env NEXT_EXPORT=true next export"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check && yarn run build"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn run lint",
      "yarn run format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.2",
    "@sentry/nextjs": "^6.9.0",
    "@tailwindcss/forms": "^0.3.3",
    "babel-plugin-styled-components": "^1.13.2",
    "gsap": "^3.7.1",
    "isomorphic-unfetch": "^3.1.0",
    "lottie-react-web": "^2.2.2",
    "next": "^11.0.1",
    "next-offline": "^5.0.5",
    "next-seo": "^4.26.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-gsap": "^3.2.1",
    "react-scrollmagic": "^2.3.0",
    "react-slick": "^0.28.1",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@next/bundle-analyzer": "^11.0.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  }
}
