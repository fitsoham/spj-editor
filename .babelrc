{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    ["@babel/plugin-proposal-optional-chaining", {}],
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".es", ".es6", ".mjs"],
        "root": ["."],
        "alias": {
          "@components": "./components",
          "@hooks": "./hooks",
          "@context": "./context",
          "@utils": "./utils",
          "@mocks": "./mocks",
          "@public": "./public"
        }
      }
    ]
  ]
}
